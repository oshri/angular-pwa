{
  "name": "angular-pwa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --base-href https://oshri.github.io/angular-pwa && ng build --prod --base-href=/angular-pwa/",
    "test": "jest --coverage",
    "test:coverage": "jest --runInBand --coverage",
    "test:snapshot": "jest --coverage -u",
    "test:watch": "jest --clearCache && jest --watch --updateSnapshot",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npx angular-cli-ghpages --dir=dist/angular-pwa --no-silent",
    "prettier": "prettier --config prettier.config.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit && npm run lint"
    }
  },
  "keywords": [
    "pwa",
    "angular",
    "github pages",
    "circleci"
  ],
  "author": "Oshri Kdoshim <cms.x.seo@gmail.com>",
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/pwa": "^0.800.1",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "^8.0.0",
    "@webcomponents/custom-elements": "^1.2.4",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.12",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.5.0",
    "husky": "^2.4.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
